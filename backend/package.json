{
  "name": "hotel-api",
  "version": "1.0.0",
  "description": "API for Hotel Data Hub",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node --esm src/server.ts'",
    "lint": "eslint . --ext .ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "create-env": "ts-node --esm src/create-env.ts",
    "create-file-tables": "node scripts/create-file-tables.js",
    "update-users-schema": "node scripts/update-users-schema.js",
    "init-permissions": "node scripts/init-permissions.js",
    "init-approval-system": "node scripts/init-approval-system.js"
  },
  "keywords": [
    "hotel",
    "api",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.808.0",
    "@aws-sdk/s3-request-presigner": "^3.808.0",
    "@prisma/client": "^6.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.6.0",
    "react-dropzone": "^14.3.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.5.0",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
